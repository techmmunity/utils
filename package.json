{
	"name": "@techmmunity/utils",
	"version": "1.8.1",
	"main": "index.js",
	"types": "index.d.ts",
	"license": "Apache-2.0",
	"author": "Techmmunity",
	"description": "Lightweight utils for every project",
	"homepage": "https://github.com/techmmunity/utils#readme",
